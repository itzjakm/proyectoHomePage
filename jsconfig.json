{
	"extends": "./.svelte-kit/tsconfig.json",
	"include": [
		"./node_modules/@sveltejs/kit/types/ambient-modules.d.ts",
		"src/**/*.d.ts",
		"src/**/*.js",
		"src/**/*.ts",
		"src/**/*.svelte"
	],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"]
		}
	}
}
